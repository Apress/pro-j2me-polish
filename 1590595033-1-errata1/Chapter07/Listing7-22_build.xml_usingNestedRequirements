<deviceRequirements>
	<or>
		<and>
			<requirement name="JavaPlatform" value="MIDP/2.0+" />
			<requirement name="SoundFormat" value="midi" />
		</and>
		<and>
			<requirement name="JavaPackage" value="nokia-ui" />
			<requirement name="BitPerPixel" value="16+" />
		</and>
	</or>
</deviceRequirements>
